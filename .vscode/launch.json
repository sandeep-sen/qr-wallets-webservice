{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Debug Nest App",
      "cwd": "${workspaceFolder}",
      "program": "${workspaceFolder}/node_modules/@nestjs/cli/bin/nest.js",
      "args": [
        "start",
        "--debug",
        "--watch"
      ],
      "restart": true,
      "protocol": "inspector",
      "console": "integratedTerminal"
    }
  ]
}
